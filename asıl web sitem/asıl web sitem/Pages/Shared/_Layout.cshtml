<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - asıl_web_sitem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/asıl_web_sitem.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.2/font/bootstrap-icons.css">
    
</head>
<body class="govde">
    <header>
        <nav class="navbar fixed-top navbar-expand-lg navbar-light bg-dark py-3 ">
            <div class="container">
                <a class="navbar-brand" href="#">
                   
                    <span id="font-logo">Avcılar Works</span>
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav ms-auto ">
                        <li class="nav-item">
                            <a class="nav-link active text-white" aria-current="page" href="#anaSayfa">AnaSayfa</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white" href="#hakkimizda">Hakkımızda</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link  text-white" href="#Urunler">Ürünler</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link  text-white" href="#Blog">Blog</a>
                        </li>
                                             
                        <li class="nav-item">
                            <a class="nav-link  text-white" href="#İletisim">İletişim</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>

    </header>
   
            @RenderBody()
        
    

    

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>